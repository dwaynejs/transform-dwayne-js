{
  "name": "transform-dwayne-js",
  "version": "5.0.7",
  "main": "lib/index.js",
  "typings": "index.d.ts",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "mocha": "^3.4.2",
    "nyc": "^11.0.3"
  },
  "scripts": {
    "build": "babel src --out-dir lib",
    "test": "nyc mocha test/index.js --reporter dot",
    "prepublish": "npm test && npm run build"
  },
  "nyc": {
    "check-coverage": true,
    "lines": 100,
    "statements": 100,
    "functions": 100,
    "branches": 100,
    "sourceMap": true,
    "include": [
      "src/**/*.js"
    ],
    "exclude": [
      "test/**/*.js",
      "node_modules/**/*"
    ],
    "reporter": [
      "html"
    ],
    "cache": true,
    "all": true
  },
  "dependencies": {
    "babel-generator": "^6.25.0",
    "babel-traverse": "^6.25.0",
    "babel-types": "^6.25.0",
    "babylon": "^6.17.4",
    "generate-code": "^2.3.2",
    "lines-and-columns": "^1.1.6",
    "lodash": "^4.17.4",
    "source-map": "^0.5.6",
    "sourcemap-codec": "^1.3.1",
    "transform-dwayne-html": "^5.9.2",
    "transform-dwayne-js-expressions": "^5.6.1"
  }
}
